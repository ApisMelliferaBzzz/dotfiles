"*":
  "color-picker":
    automaticReplace: true
  core:
    autoHideMenuBar: true
    disabledPackages: [
      "minimap"
      "vim-mode"
      "vim-mode-plus"
    ]
    telemetryConsent: "no"
    themes: [
      "one-dark-ui"
      "beta-syntax"
    ]
  editor:
    backUpBeforeSaving: true
    fontFamily: "Roboto Mono"
    fontSize: 17
    invisibles: {}
    lineHeight: 1.2
    scrollPastEnd: true
    showIndentGuide: true
  "exception-reporting":
    userId: "f336575b-64be-6bf1-ae6d-1fb756de4913"
  "one-dark-ui": {}
  welcome:
    showOnStartup: false
